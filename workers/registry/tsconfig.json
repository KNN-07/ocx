{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "bundler",
		"lib": ["ESNext"],
		"types": ["bun-types"],
		"strict": true,
		"skipLibCheck": true,
		"noEmit": true,
		"isolatedModules": true,
		"allowImportingTsExtensions": true
	},
	"include": ["src/**/*", "worker-configuration.d.ts"]
}

{
  "name": "reviewer",
  "versions": {
    "1.0.0": {
      "name": "reviewer",
      "type": "ocx:agent",
      "description": "Code review specialist that analyzes code for issues and improvements",
      "files": [
        "agent/reviewer.md"
      ],
      "dependencies": [
        "code-review",
        "plan-review"
      ],
      "opencode": {
        "agent": {
          "reviewer": {
            "temperature": 0.1,
            "permission": {
              "edit": "deny",
              "write": "deny",
              "bash": {
                "*": "deny",
                "git diff*": "allow",
                "git log*": "allow",
                "git show*": "allow",
                "git blame*": "allow",
                "rg *": "allow"
              }
            }
          }
        }
      }
    }
  },
  "dist-tags": {
    "latest": "1.0.0"
  }
}
{
	"name": "agentcn",
	"version": "0.1.0",
	"private": true,
	"description": "ShadCN-style registry for AI coding agents",
	"author": "kdcokenny",
	"license": "MIT",
	"packageManager": "bun@1.3.5",
	"repository": {
